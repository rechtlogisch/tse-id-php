#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Rechtlogisch\TseId\RetrieveCommand;

$application = new Application();

$application->add(new RetrieveCommand);

try {
    $application->run();
} catch (Exception $e) {
    echo $e->getMessage();
    exit(1);
}
